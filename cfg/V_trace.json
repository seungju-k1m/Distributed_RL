{
    "envName":"Breakout-v0",

    "replayMemory":200000,
    "batchSize":32,
    "RewardClipping":[-1, 1],
    "grayscale":true,
    "gamma":0.99,
    "EntropyRegularizer":0.01,
    "clipingGradientNorm":40,
    "stack":4,
    "actionSize":4,
    "stateSize":[4, 84, 84],
    "unroll_step":20,

    "sPath":"./save/V_Trace/",
    "writeTMode":false,
    "tPath":"./tensorboard/V_Trace/BreakOut-v0",
    "lPath":false,
    "leanerDevice":"cuda:0",
    "actorDevice":"cuda:0",
    "hostName":"localhost",

    "optim":{
        "actor-critic":{
            "name":"rmsprop",
            "lr":0.0006,
            "momentum":0,
            "epsilon":0.01
        }

    },
    "agent":{
        "actor-critic":{
            "module00":{
                "netCat":"CNN2D",
                "iSize":4,
                "nLayer":3,
                "fSize":[8, 4, -1],
                "nUnit":[16, 32],
                "padding":[0, 0],
                "stride":[4, 2],
                "act":["relu", "relu"],
                "BN":[false, false, false],
                "linear":true,
                "input":[0],
                "prior":0
            }
        }
    }
}