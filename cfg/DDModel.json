{
    "envName":"./Env/v0",
    "timeScale":5,
    "env":{
        "timeStep":0.2,
        "horizonTime":3
    },
    "agent":{
        "Embedded":{
            "module00":{
                "netCat":"CNN2D",
                "iSize":3,
                "nLayer":3,
                "fSize":[5, 3, 3, -1],
                "nUnit":[32, 64, 64],
                "stride":[2, 2, 2,2],
                "act":["relu", "relu","relu"],
                "padding":[0, 0, 0,0],
                "BN":[false, false, false,false],
                "linear":true,
                "input":[0],
                "prior":0
            },
            "module01":{
                "netCat":"MLP",
                "iSize":289536,
                "nLayer":4,
                "fSize":[256, 256, 128, 128],
                "act":["relu", "relu", "relu", "linear"],
                "BN":false,
                "prior":1,
                "prevNodeNames":["module00"],
                "output":true
            }
        },
        "Output":{
            "module00":{
                "netCat":"MLP",
                "iSize":2,
                "nLayer":2,
                "fSize":[16, 16],
                "act":["relu", "linear"],
                "prior":0,
                "BN":false,
                "input":[0]
            },
            "module01":{
                "netCat":"View",
                "shape":[1, -1, 16],
                "prior":1,
                "prevNodeNames":["module00"]
            },
            "module02":{
                "netCat":"LSTMNET",
                "iSize":16,
                "nLayer":1,
                "hiddenSize":64,
                "FlattenMode":true,
                "device":"cpu",
                "prior":2,
                "prevNodeNames":["module01"]
            },
            "module03":{
                "netCat":"MLP",
                "iSize":64,
                "nLayer":2,
                "fSize":[64, 2],
                "act":["relu", "linear"],
                "BN":false,
                "prior":3,
                "prevNodeNames":["module02"]
            },
            "module04":{
                "netCat":"MLP",
                "iSize":64,
                "nLayer":2,
                "fSize":[64, 1],
                "act":["relu", "sigmoid"],
                "BN":false,
                "prior":3,
                "prevNodeNames":["module02"]
            },
            "module05":{
                "netCat":"Cat",
                "prior":4,
                "prevNodeNames":["module03", "module04"],
                "output":true
            }

        }
    },
    "optim":{
        "model":{
            "name":"adam",
            "lr":1e-4,
            "decay":0.001
        }
    },
    "replayMemory":50,
    "batchSize":32,
    "gradientNorm":1000000,

    "learnerDevice":"cuda:0",
    "playerDevice":"cuda:0",

    "sPath":"./save/DDModel/v0.pth",
    "writeTMode":false,
    "tPath":"./tensorboard/DDModel/v0",
    "dataPath":"./TestData/",

    "GMP_theta":0.5,
    "GMP_deltaT":0.05,
    "GMP_drift":[0.07, 0],
    "GMP_sigma":0.03,

    "GMP_yaw_theta":1.0,
    "GMP_yaw_deltaT":0.1,
    "GMP_yaw_sigma":0.1
}