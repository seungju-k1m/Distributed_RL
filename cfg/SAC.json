{
    "envName":"InvertedPendulum-v2",
    "gamma": 0.99,
    "tau":0.005,
    "updateInterval":1000,
    "runStep":300e3,
    "startStep":1e4,
    "actionSize":1,
    "fixedTemp":true,
    "tempValue":0.2,
    "alpha":0.6,
    "beta0":0.4,
    "betaDecay":1000000,

    "replayMemory":1e6,
    "batchSize":256,
    "learningFrequency":1,
    "rScaling":20,
    "sPath":"./save/SAC/",
    "writeTMode":false,
    "tPath":"./tensorboard/SAC/",
    "lPath":false,
    "device":"cpu",
    "hostName":"localhost",

    "optim":{
        "actor":{
            "name":"adam",
            "lr":3e-4,
            "eps":1e-7
        },
        "critic":{
            "name":"adam",
            "lr":3e-4,
            "eps":1e-7
        },
        "temperature":{
            "name":"adam",
            "lr":3e-4,
            "eps":1e-7
        }
    },
    "agent":{
        "actor":{
            "module00":{
                "netCat":"MLP",
                "iSize":4,
                "nLayer":3,
                "fSize":[256, 256, 2],
                "act":["relu", "relu", "linear"],
                "BN":false,
                "output":true,
                "input":[0],
                "prior":0
            }
        },
        "critic":{
            "module00":{
                "netCat":"MLP",
                "iSize":5,
                "nLayer":3,
                "fSize":[256, 256, 1],
                "act":["relu", "relu", "linear"],
                "BN":false,
                "output":true,
                "input":[0],
                "prior":0
            }
        }

    }
}