{
    "ALG":"APE_X",

    "REDIS_SERVER_":"10.86.230.140",
    "REDIS_SERVER_PUSH":"localhost",
    "REDIS_SERVER":"localhost",

    "ACTION_SIZE":6,

    "_PER_":true,
    "ALPHA":0.6,
    "BETA":0.4,
    "GAMMA":0.99,
    "TARGET_FREQUENCY":2500,
    "N":32,
    "BATCHSIZE":32,

    "DEVICE":"cpu",
    "LEARNER_DEVICE":"cuda:0",

    "REPLAY_MEMORY_LEN":1000000,
    "BUFFER_SIZE":50000,
    "UNROLL_STEP":3,

    "USE_REWARD_CLIP":true,

    "optim":{
        "name":"rmsprop",
        "lr":0.0000625,
        "eps":1.5e-7,
        "decay":0,
        "alpha":0.95,
        "momentum":0,
        "centered":true
    },

    "model":{
        "module00":{
            "netCat":"CNN2D",
            "iSize":4,
            "nLayer":4,
            "fSize":[8, 4, 3, -1],
            "nUnit":[32, 64, 64],
            "padding":[0, 0, 0],
            "stride":[4, 2, 1],
            "act":["relu", "relu", "relu"],
            "BN":[false, false, false, false],
            "linear":true,
            "input":[0],
            "prior":0
        },
        "module02":{
            "netCat":"MLP",
            "iSize":3136,
            "nLayer":2,
            "fSize":[512, 6],
            "act":["relu", "linear"],
            "BN":[false, false, false],
            "prior":1,
            "prevNodeNames":["module00"]
        },
        "module02_1":{
            "netCat":"MLP",
            "iSize":3136,
            "nLayer":2,
            "fSize":[512,  1],
            "act":["relu",  "linear"],
            "BN":[false, false, false],
            "prior":1,
            "prevNodeNames":["module00"]
        },
        "module03":{
            "netCat":"Add",
            "prior":2,
            "prevNodeNames":["module02", "module02_1"]
        },
        "module03_1":{
            "netCat":"Mean",
            "prior":2,
            "prevNodeNames":["module02"]
        },
        "module04":{
            "netCat":"Substract",
            "prior":3,
            "prevNodeNames":["module03", "module03_1"],
            "output":true
        }
    }
        
}
    
