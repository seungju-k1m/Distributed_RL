{
    "envName":"Breakout-v4",

    "lstmMode":false,

    "replayMemory":10000,
    "startMemory":500,
    "batchSize":32,
    "RewardClipping":[-1, 1],
    "grayscale":true,
    "gamma":0.99,
    "c_lambda":0.997,
    "c_value":1.0,
    "p_value":1.0,
    "EntropyRegularizer":0.01,
    "clipingGradientNorm":40,
    "stack":4,
    "actionSize":4,
    "stateSize":[4, 84, 84],
    "unroll_step":20,
    "gradientNorm":40,

    "sPath":"./save/IMPALA/",
    "writeTMode":true,
    "tPath":"./tensorboard/IMPALA/BreakOut-v4",
    "lPath":false,
    "learnerDevice":"cuda:0",
    "actorDevice":"cuda:0",
    "hostName":"localhost",
    "renderMode":false,

    "optim":{
        "actor-critic":{
            "name":"rmsprop",
            "lr":0.0006,
            "momentum":0,
            "eps":0.01
        }

    },
    "agent":{
        "actor-critic":{
            "module00":{
                "netCat":"CNN2D",
                "iSize":4,
                "nLayer":3,
                "fSize":[8, 4, -1],
                "nUnit":[16, 32],
                "padding":[0, 0],
                "stride":[4, 2],
                "act":["relu", "relu"],
                "BN":[false, false, false],
                "linear":true,
                "input":[0],
                "prior":0
            },
            "module01":{
                "netCat":"MLP",
                "iSize":2592,
                "nLayer":2,
                "fSize":[256, 5],
                "act":["relu", "linear"],
                "BN":false,
                "prior":1,
                "prevNodeNames":["module00"],
                "output":true
            }
        }
    }
}
