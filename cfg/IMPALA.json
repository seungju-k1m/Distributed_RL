{
    "envName":"Breakout-v4",

    "replayMemory":200000,
    "startMemory":100,
    "batchSize":32,
    "RewardClipping":[-1, 1],
    "grayscale":true,
    "gamma":0.99,
    "c_lambda":0.997,
    "c_value":1.0,
    "p_value":1.0,
    "EntropyRegularizer":0.01,
    "clipingGradientNorm":40,
    "stack":4,
    "actionSize":4,
    "stateSize":[4, 84, 84],
    "unroll_step":20,
    "gradientNorm":40,

    "sPath":"./save/V_Trace/",
    "writeTMode":false,
    "tPath":"./tensorboard/V_Trace/BreakOut-v0",
    "lPath":false,
    "leanerDevice":"cpu",
    "actorDevice":"cpu",
    "hostName":"localhost",

    "optim":{
        "actor-critic":{
            "name":"rmsprop",
            "lr":0.0006,
            "momentum":0,
            "epsilon":0.01
        }

    },
    "agent":{
        "actor-critic":{
            "module00":{
                "netCat":"CNN2D",
                "iSize":4,
                "nLayer":3,
                "fSize":[8, 4, -1],
                "nUnit":[16, 32],
                "padding":[0, 0],
                "stride":[4, 2],
                "act":["relu", "relu"],
                "BN":[false, false, false],
                "linear":true,
                "input":[0],
                "prior":0
            },
            "module01":{
                "netCat":"MLP",
                "iSize":2592,
                "nLayer":1,
                "fSize":[256],
                "act":["relu"],
                "BN":false,
                "prior":1,
                "prevNodeNames":["module00"]
            },
            "module02":{
                "netCat":"View",
                "shape":[-1, 1, 256],
                "prior":2,
                "prevNodeNames":["module01"]
            },
            "module03":{
                "netCat":"LSTMNET",
                "iSize":256,
                "nLayer":1,
                "hiddenSize":256,
                "FlattenMode":true,
                "device":"cpu",
                "prior":3,
                "Number_Agent":1,
                "prevNodeNames":["module02"]
            },
            "module04":{
                "netCat":"MLP",
                "iSize":256,
                "nLayer":1,
                "fSize":[5],
                "act":["linear"],
                "BN":false,
                "output":true,
                "prevNodeNames":["module03"],
                "prior":4
            }
        }
    }
}